	"tokenColors": [
		{
			"name": "Function declarations",
			"scope": [
				"entity.name.function",
				"support.function",
				"support.constant.handlebars",
				"source.powershell variable.other.member",
				"entity.name.operator.custom-literal"
			],
			"settings": {
				"foreground": "#0A84FF"
			}
		},
		{
			"name": "Types declaration and references",
			"scope": [
				"support.class",
				"support.type",
				"entity.name.type",
				"entity.name.namespace",
				"entity.other.attribute",
				"entity.name.scope-resolution",
				"entity.name.class"
			],
			"settings": {
				"foreground": "#FF9F0A"
			}
		},
		{
			"name": "Control flow / Keywords",
			"scope": [
				"keyword.control",
				"keyword.operator.new",
				"keyword.operator.expression",
				"keyword.cast",
				"keyword.operator.expression",
				"keyword.other.new",
				"keyword.other.unit",
				"keyword.other.ternary"
			],
			"settings": {
				"foreground": "#BF5AF2"
			}
		},
		{
			"name": "Variables and parameters",
			"scope": [
				"variable",
				"entity.name.variable",
				"support.variable",
				"variable.other.readwrite",
				"variable.parameter"
			],
			"settings": {
				"foreground": "#A1A1A6"
			}
		},
		{
			"name": "Strings",
			"scope": [
				"string",
				"constant.other.symbol",
				"constant.other.key",
				"support.constant.property-value"
			],
			"settings": {
				"foreground": "#FFCC00"
			}
		},
		{
			"name": "Numbers",
			"scope": [
				"constant.numeric",
				"constant.language",
				"support.constant.math",
				"constant.color",
				"constant.numeric.integer",
				"constant.numeric.float",
				"constant.numeric.hex",
				"constant.numeric.octal",
				"constant.numeric.binary",
				"constant.numeric.decimal"
			],
			"settings": {
				"foreground": "#4FD649"
			}
		},
		{
			"name": "Comments",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"foreground": "#86868B80"
			}
		},
		{
			"name": "Constants",
			"scope": [
				"constant",
				"support.constant",
				"variable.other.constant",
				"variable.other.enummember"
			],
			"settings": {
				"foreground": "#BF5AF2"
			}
		},
		{
			"name": "Operators",
			"scope": [
				"keyword.operator",
				"punctuation.accessor",
				"meta.brace.square",
				"meta.delimiter.object.comma",
				"meta.delimiter.method.period",
				"meta.tag",
				"punctuation.definition.tag",
				"punctuation.definition.tag.begin",
				"punctuation.definition.tag.end",
				"punctuation.separator.key-value"
			],
			"settings": {
				"foreground": "#6E6E73"
			}
		},
		{
			"name": "Tags and attributes",
			"scope": [
				"entity.name.tag",
				"meta.tag",
				"entity.other.attribute-name",
				"punctuation.definition.tag.begin",
				"punctuation.definition.tag.end"
			],
			"settings": {
				"foreground": "#4FD649"
			}
		},
		{
			"name": "Properties and methods",
			"scope": [
				"entity.other.inherited-class",
				"meta.function-call",
				"variable.function",
				"variable.other.property",
				"support.function",
				"support.method",
				"meta.method-call"
			],
			"settings": {
				"foreground": "#0A84FF"
			}
		},
		{
			"name": "Built-in objects",
			"scope": [
				"variable.language",
				"support.variable",
				"variable.other.object"
			],
			"settings": {
				"foreground": "#FF9F0A"
			}
		},
		{
			"name": "CSS and style definitions",
			"scope": [
				"support.type.property-name",
				"support.function.misc",
				"meta.property-name",
				"entity.name.function"
			],
			"settings": {
				"foreground": "#4FD649"
			}
		},
		{
			"name": "Regular expressions",
			"scope": [
				"string.regexp",
				"keyword.other.regexp"
			],
			"settings": {
				"foreground": "#FF78D4"
			}
		},
		{
			"name": "Markup",
			"scope": [
				"markup.heading",
				"markup.bold",
				"markup.italic",
				"markup.underline",
				"markup.strikethrough"
			],
			"settings": {
				"foreground": "#BF5AF2",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Emphasis",
			"scope": [
				"emphasis"
			],
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"name": "Invalid and deprecated",
			"scope": [
				"invalid",
				"invalid.deprecated",
				"variable.parameter.invalid",
				"keyword.other.doc"
			],
			"settings": {
				"foreground": "#FF453A"
			}
		}
	]
}